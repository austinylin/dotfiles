[user]
  name = Austin Lin
  email = austinylin@gmail.com
[github]
  user = austinylin
[push]
  default = current
[core]
  excludesfile = ~/.gitignore_global
  editor = vim
[alias]
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[color]
  ui = true
[init]
  defaultBranch = main
